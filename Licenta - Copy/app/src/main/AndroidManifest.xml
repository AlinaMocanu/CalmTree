<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.calmtree">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.thought.SavedHandout3" />
        <activity android:name=".ui.thought.SavedHandout2" />
        <activity android:name=".ui.thought.SavedHandout" />
        <activity
            android:name=".ui.thought.AllHandouts3"
            android:label="@string/title_activity_all_handouts3"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.thought.AllHandouts2"
            android:label="@string/title_activity_all_handouts2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.thought.AllHandouts"
            android:label="@string/title_activity_all_handouts"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.thought.NewHandout3" />
        <activity android:name=".ui.thought.NewHandout2" />
        <activity android:name=".ui.thought.NewHandout" />
        <activity
            android:name=".ui.thought.NewHandoutActivity3"
            android:label="@string/title_activity_new_handout3"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.thought.NewHandoutActivity2"
            android:label="@string/title_activity_new_handout2"
